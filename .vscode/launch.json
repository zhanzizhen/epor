  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch index.js",
      "program": "${workspaceFolder}/lib/index.js",
      "request": "launch",
      "skipFiles": ["<node_internals>/**"],
      "type": "node",
      "args": ["generate", "-t"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch index.ts",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}\\src\\index.ts",
      "outFiles": ["${workspaceFolder}/**/*.js"]
    }
  ]
}
